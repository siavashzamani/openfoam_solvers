FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
    sphereToCell
    {
        centre       (0.002 0.002 0.002);
        radius       0.0015;

        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);


