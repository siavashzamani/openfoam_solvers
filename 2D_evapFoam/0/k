FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.01;

boundaryField
{

    leftWall
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    rightWall
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    topAndBottom
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
